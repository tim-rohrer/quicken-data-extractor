{
  "name": "Quicken Data Extractor",
  "version": "0.1.0-a",
  "description": "Extracts user-specified fields from a personal Quicken (Mac) datafile; migrates and normalizes data based on new field names",
  "main": "index.js",
  "scripts": {
    "build": "yarn clean:build && babel src --ignore src/**/*spec* --out-dir lib --extensions '.ts,.tsx'",
    "clean:build": "rm -rf lib",
    "test-coverage": "jest --coverage",
    "type-check": "tsc",
    "test": "jest --watch",
    "linter": "eslint src --ext ts --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tim-rohrer/quicken-data-extractor.git"
  },
  "keywords": [
    "template",
    "node",
    "local",
    "module"
  ],
  "author": "Tim Rohrer",
  "license": "GPLv3",
  "bugs": {
    "url": "https://github.com/tim-rohrer/quicken-data-extractor/issues"
  },
  "homepage": "https://github.com/tim-rohrer/quicken-data-extractor#readme",
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@types/express": "^4.17.6",
    "@types/jest": "^26.0.3",
    "@types/pluralize": "^0.0.29",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "babel-jest": "^26.1.0",
    "babel-loader": "^8.1.0",
    "eslint": "^7.4.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "jest": "^26.1.0",
    "typescript": "^3.9.6"
  },
  "dependencies": {
    "pluralize": "^8.0.0",
    "sqlite": "^4.0.15",
    "sqlite3": "^5.0.0"
  }
}
