{
  "name": "quicken-data-extractor",
  "version": "0.3.0-a",
  "description": "Extracts user-specified fields from a personal Quicken (Mac) data file; migrates and normalizes data based on new field names",
  "main": "bin/index.js",
  "types": "bin/index.d.ts",
  "scripts": {
    "build": "npm run build:clean && npm run build:types && npm run build:js",
    "build:clean": "rm -rf bin",
    "build:types": "tsc",
    "build:js": "babel src --ignore src/**/tests/** --out-dir bin --extensions '.ts,.tsx'",
    "test-coverage": "jest --coverage",
    "type-check": "tsc",
    "test": "jest --watch",
    "linter": "eslint src --ext ts --fix",
    "eslint:check": "eslint --print-config src/index.ts | eslint-config-prettier-check"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tim-rohrer/quicken-data-extractor.git"
  },
  "keywords": [
    "template",
    "node",
    "local",
    "module"
  ],
  "author": "Tim Rohrer",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/tim-rohrer/quicken-data-extractor/issues"
  },
  "homepage": "https://github.com/tim-rohrer/quicken-data-extractor#readme",
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@tsconfig/node16": "^1.0.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/pluralize": "^0.0.29",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "babel-jest": "^27.3.1",
    "babel-loader": "^8.2.3",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.2.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "supertest": "^6.1.6",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "pluralize": "^8.0.0",
    "sqlite": "^4.0.23",
    "sqlite3": "^5.0.2"
  }
}
